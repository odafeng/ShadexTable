{
  "name": "ShadexTable",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:no-lint": "next build --no-lint",
    "start": "next start",
    "// Linting & Type Checking": "",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "next lint --strict",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run lint",
    "// Formatting": "",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "// Code Cleaning": "",
    "clean:imports": "eslint . --fix --rule 'unused-imports/no-unused-imports: error'",
    "clean:vars": "eslint . --fix",
    "clean:all": "npm run clean:imports && npm run format",
    "clean:full": "npm run lint:fix && npm run format && npm run typecheck",
    "// Testing": "",
    "test": "vitest",
    "test:api": "vitest tests/api",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "// Git Hooks": "",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "npm run verify && npm run test:run",
    "// Utilities": "",
    "check:all": "npm run typecheck && npm run lint && npm run format:check",
    "fix:all": "npm run clean:all && npm run typecheck"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run verify"
    }
  },
